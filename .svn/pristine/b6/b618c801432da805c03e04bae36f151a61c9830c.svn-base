package com.zhongz.rental.domain;

import java.util.Date;

import org.apache.commons.lang3.builder.ToStringBuilder;
import org.apache.commons.lang3.builder.ToStringStyle;

import com.ruoyi.common.core.domain.BaseEntity;

/**
 * 意见反馈表 zhongz_suggestion
 * 
 * @author ruoyi
 * @date 2019-04-28
 */
public class ZhongzSuggestion extends BaseEntity
{
	private static final long serialVersionUID = 1L;
	
	/** 主键 */
	private String id;
	/** 反馈人id */
	private String userId;
	/** 反馈时间 */
	private Date suggestTime;
	/** 反馈内容 */
	private String content;
	/** 受理状态 */
	private Integer acceptStatus;
	/** 受理人id */
	private String acceptorId;
	/** 受理时间 */
	private Date acceptTime;
	/** 受理描述 */
	private String description;
	/** 创建者 */
	private String createBy;
	/** 创建时间 */
	private Date createTime;
	/** 更新者 */
	private String updateBy;
	/** 更新时间 */
	private Date updateTime;
	/** 备注 */
	private String remark;
	/** 状态（0正常 1删除 2停用） */
	private String status1;
	
	/** 用户昵称 */
	private String nickName;
	/** 用户手机号 */
	private String mobile;
	/** 受理人员 */
	private String userName;
	/** 主键id数组 */
	private String[] ids;

	public void setId(String id) 
	{
		this.id = id;
	}

	public String getId() 
	{
		return id;
	}
	public void setUserId(String userId) 
	{
		this.userId = userId;
	}

	public String getUserId() 
	{
		return userId;
	}
	public void setSuggestTime(Date suggestTime) 
	{
		this.suggestTime = suggestTime;
	}

	public Date getSuggestTime() 
	{
		return suggestTime;
	}
	public void setContent(String content) 
	{
		this.content = content;
	}

	public String getContent() 
	{
		return content;
	}
	public void setAcceptStatus(Integer acceptStatus) 
	{
		this.acceptStatus = acceptStatus;
	}

	public Integer getAcceptStatus() 
	{
		return acceptStatus;
	}
	public void setAcceptorId(String acceptorId) 
	{
		this.acceptorId = acceptorId;
	}

	public String getAcceptorId() 
	{
		return acceptorId;
	}
	public void setAcceptTime(Date acceptTime) 
	{
		this.acceptTime = acceptTime;
	}

	public Date getAcceptTime() 
	{
		return acceptTime;
	}
	public void setDescription(String description) 
	{
		this.description = description;
	}

	public String getDescription() 
	{
		return description;
	}
	public void setCreateBy(String createBy) 
	{
		this.createBy = createBy;
	}

	public String getCreateBy() 
	{
		return createBy;
	}
	public void setCreateTime(Date createTime) 
	{
		this.createTime = createTime;
	}

	public Date getCreateTime() 
	{
		return createTime;
	}
	public void setUpdateBy(String updateBy) 
	{
		this.updateBy = updateBy;
	}

	public String getUpdateBy() 
	{
		return updateBy;
	}
	public void setUpdateTime(Date updateTime) 
	{
		this.updateTime = updateTime;
	}

	public Date getUpdateTime() 
	{
		return updateTime;
	}
	public void setRemark(String remark) 
	{
		this.remark = remark;
	}

	public String getRemark() 
	{
		return remark;
	}
	public void setStatus1(String status1) 
	{
		this.status1 = status1;
	}

	public String getStatus1() 
	{
		return status1;
	}
	
    public String getNickName() {
		return nickName;
	}

	public void setNickName(String nickName) {
		this.nickName = nickName;
	}
	
	public String getMobile() {
		return mobile;
	}

	public void setMobile(String mobile) {
		this.mobile = mobile;
	}
	
	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}
	
	public String[] getIds() {
		return ids;
	}

	public void setIds(String[] ids) {
		this.ids = ids;
	}

	public String toString() {
        return new ToStringBuilder(this,ToStringStyle.MULTI_LINE_STYLE)
            .append("id", getId())
            .append("userId", getUserId())
            .append("suggestTime", getSuggestTime())
            .append("content", getContent())
            .append("acceptStatus", getAcceptStatus())
            .append("acceptorId", getAcceptorId())
            .append("acceptTime", getAcceptTime())
            .append("description", getDescription())
            .append("createBy", getCreateBy())
            .append("createTime", getCreateTime())
            .append("updateBy", getUpdateBy())
            .append("updateTime", getUpdateTime())
            .append("remark", getRemark())
            .append("status1", getStatus1())
            .append("nickName", getNickName())
            .append("mobile", getMobile())
            .append("userName", getUserName())
            .append("ids", getIds())
            .toString();
    }
}
